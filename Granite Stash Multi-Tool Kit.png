            if (onlineStoreInputForm.style['display'] === 'block') {
                var onlineValidity = validateOnlineInputs(onlineUrl, onlinePriceRaw, onlineShippingRaw, errorMessages);
                if (onlineValidity.valid) {
                    cleanRetailerType = 'ONLINE';
