                    cleanSampleDate = formatDate(
                        currentYear,
                        onlineDay ? onlineDay : getCurrentDay(),
                        onlineMonth ? onlineMonth : getCurrentMonth()
                    );
                } else {
                    if (onlineValidity.error) {
                        errorDiv.innerHTML = getErrorDiv(onlineValidity.error);
