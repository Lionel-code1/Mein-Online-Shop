            if (!instance) {
                var options = {
                    name: "pricingFeedback-modal-content",
                    dataStrategy: "preload",
                };
                instance = modal.create(event.$target, options);
            }
            instance.update({
                header: title,
                width: getModalWidth()
            }).lock().show();
        });
        $(window).resize(function() {
            if (instance) {
                instance.update({
                    width: getModalWidth()
                });
            }
        });
    });


